  "scripts": {
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "check": "npm run test && npm run lint && echo \"\\033[0;32mReady to publish\\033[0m\"",
    "gulp": "gulp",
    "compile": "NODE_ENV=production npm run gulp compile",
    "prepublishOnly": "npm run check && npm run compile",
    "lint": "stylint ./ && eslint --ext .es,.js ./",
    "lint:tofile": "eslint --ext .es,.js --no-color -o ./eslint-report.log ./",
    "test": "npm run compile && TZ='GMT' mocha \"test/**/*.js\"",
    "test:tofile": "npm run prepublishOnly && TZ='GMT' mocha --no-colors -R spec \"test/**/*.js\" 2>&1 | tee test-report.log",
    "start": "NODE_ENV=production npm run gulp"
  },
